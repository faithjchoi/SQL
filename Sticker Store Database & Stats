--display the database ordered by Primary Key
SELECT * FROM store;

--display the database ordered by quantity descending
SELECT * FROM store WHERE Quantity > 95 ORDER BY quantity DESC;

--display the database ordered by dollar amount descending
SELECT * FROM store WHERE dollars > 0 ORDER by dollars DESC;

--display the database ordered in alphabetical order
SELECT * FROM store ORDER BY Name 
